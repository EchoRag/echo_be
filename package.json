{
  "name": "echo_be",
  "version": "0.0.9",
  "description": "Echo Backend Service",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "predockerbuild": "npm run build",
    "dockerbuild": "docker build --platform linux/x86_64 -t echo_be .",
    "predockerTag": "npm run dockerbuild",
    "dockerTag": "docker tag echo_be:latest asia-south1-docker.pkg.dev/echo-rag/echo/echo_be:$npm_package_version",
    "prepushGcp": "npm run dockerTag",
    "pushGcp": "docker push asia-south1-docker.pkg.dev/echo-rag/echo/echo_be:$npm_package_version",
    "test:version": "echo 'Testing version handling:' && echo 'Package version:' $npm_package_version && echo 'Docker tag:' asia-south1-docker.pkg.dev/echo-rag/echo/echo_be:$npm_package_version",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate -- -d src/config/database.ts",
    "migration:run": "npm run typeorm migration:run -- -d src/config/database.ts",
    "migration:revert": "npm run typeorm migration:revert -- -d src/config/database.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@clerk/clerk-sdk-node": "^4.0.0",
    "@clerk/express": "^1.3.56",
    "@google-cloud/recaptcha-enterprise": "^6.0.1",
    "@google-cloud/storage": "^7.16.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.57.1",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@opentelemetry/sdk-trace-node": "^2.0.0",
    "@types/amqplib": "^0.10.7",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.15.11",
    "@types/swagger-jsdoc": "^6.0.1",
    "@types/swagger-ui-express": "^4.1.3",
    "amqplib": "^0.10.5",
    "axios": "^1.8.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.7.0",
    "firebase-admin": "^13.2.0",
    "helmet": "^6.0.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "pg": "^8.10.0",
    "reflect-metadata": "^0.1.13",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "typeorm": "^0.3.15",
    "typescript": "^5.0.3",
    "winston": "^3.17.0",
    "winston-loki": "^6.1.3"
  },
  "devDependencies": {
    "@types/multer": "^1.4.12",
    "@types/supertest": "^6.0.2",
    "jest": "^29.5.0",
    "pm2": "^6.0.5",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typeorm-ts-node-commonjs": "^0.3.20"
  }
}
